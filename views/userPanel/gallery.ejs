<%- include('user_header') %>
    <section class="page-heading wow fadeIn" data-wow-duration="1.5s"
        style="background-image: url(files/images/01-heading.jpg)">
        <div class="container">
            <div class="page-name">
                <h1>Latest Photos</h1>
                <span>Lovely layout of heading</span>
            </div>
        </div>
    </section>

    <section class="portfolio on-portfolio">
        <div class="container">
            <div class="col-sm-12 text-center">
                <div id="projects-filter">
                    <a href="#" data-filter="*" class="active">Show All</a>
                    <% for(var al of allData){ %>
                        <a href="#" data-filter=".<%= al.id %>">
                            <%= al.category %>
                        </a>
                        <% } %>

                </div>
            </div>
            <div class="row">
                <div class="row" id="portfolio-grid">
                    <% for(var s of subD){ %>
                        <div class="item col-md-3 col-sm-6 col-xs-12 <%= s.categoryId.id %>">
                            <figure>
                                <img alt="1-image" src="<%= s.subImage %>">
                                <figcaption>
                                    <h3>Plaid Authentic</h3>
                                    <p>Lorem ipsum dolor sit amet consete.</p>
                                </figcaption>
                            </figure>
                        </div>
                        <% } %>

                </div>
            </div>
            
        </div>
    </section>
    <%- include('user_footer') %>